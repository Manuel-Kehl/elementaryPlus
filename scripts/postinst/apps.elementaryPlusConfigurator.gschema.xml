<?xml version="1.0" encoding="UTF-8"?>
<schemalist>
  <schema id="apps.elementaryPlusConfigurator" path="/apps/elementaryPlusConfigurator/">
    <key type="as" name="installed">
      <default>[]</default>
      <summary>Installed components</summary>
      <description>List of installed components.</description>
    </key>
    <key type="b" name="sniqt-patched">
      <default>false</default>
      <summary>Sni-qt patched</summary>
      <description>True if patched sni-qt installed.</description>
    </key>
    <key type="s" name="previous-icon-theme">
      <default>""</default>
      <summary>Previous icon theme</summary>
      <description>The name of the icon theme used before selecting elementary+</description>
    </key>
    <key type="b" name="first-run">
      <default>true</default>
      <summary>First run</summary>
      <description>First run.</description>
    </key>
   </schema>
</schemalist>

